  {
    "name": "Simplest timer rxjs",
    "version": "0.2",
    "description": "The most simple timer",
    "permissions": ["declarativeContent", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/stopwatch_16.png",
        "32": "images/stopwatch_32.png",
        "48": "images/stopwatch_48.png",
        "128": "images/stopwatch_128.png"
      }
    },
    "icons": {
        "16": "images/stopwatch_16.png",
        "32": "images/stopwatch_32.png",
        "48": "images/stopwatch_48.png",
        "128": "images/stopwatch_128.png"
    },
    "manifest_version": 2
  }